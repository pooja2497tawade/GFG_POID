class Solution 
{ 
    static int xmod11(String x)
	{    
	    // code here
	    
	     int num=x.charAt(0)-48;
       
       int i=1;
       while(i<x.length()){
           num=num*10+x.charAt(i)-48;
           int temp=num%11;
           num=temp;
           i++;
       }
       return num;

	}
} 
